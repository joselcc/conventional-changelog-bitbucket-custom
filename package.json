{
  "name": "conventional-changelog-bitbucket-custom",
  "version": "1.0.1",
  "description": "conventional-changelog customized preset for BitBucket",
  "keywords": [
    "angular",
    "bitbucket",
    "changelog",
    "conventional-changelog",
    "preset"
  ],
  "license": "MIT",
  "files": ["src"],
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:check-coverage": "nyc check-coverage",
    "test:coverage": "npm-run-all test:unit:once test:check-coverage --silent",
    "test:unit":
      "cross-env NODE_ENV=test nyc mocha --opts config/testUnit/mocha.opts -w",
    "test:unit:once":
      "cross-env NODE_ENV=test nyc mocha --opts config/testUnit/mocha.opts"
  },
  "dependencies": {
    "compare-func": "1.3.1",
    "q": "1.4.1"
  },
  "devDependencies": {
    "better-than-before": "1.0.0",
    "chai": "3.4.1",
    "chokidar-cli": "1.2.0",
    "conventional-changelog-core": "1.8.0",
    "coveralls": "2.13.1",
    "cross-env": "3.1.4",
    "cz-customizable": "5.2.0",
    "cz-customizable-ghooks": "1.5.0",
    "eslint": "4.5.0",
    "eslint-config-google": "0.9.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-plugin-node": "5.1.1",
    "git-dummy-commit": "1.1.1",
    "husky": "0.13.3",
    "mocha": "3.5.0",
    "mocha-junit-reporter": "1.13.0",
    "mocha-multi-reporters": "1.1.5",
    "npm-run-all": "4.0.2",
    "nyc": "11.1.0",
    "rimraf": "2.6.1",
    "semantic-release": "7.0.2",
    "shelljs": "0.7.3",
    "through2": "2.0.0"
  },
  "engines": {
    "node": ">=4.x"
  }
}
