{
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 90,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.spec.js"
    ],
    "reporter": [
      "lcovonly",
      "html",
      "text",
      "cobertura",
      "json"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./reports/coverage/"
}
